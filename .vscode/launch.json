{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Debug Veturpack",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceRoot}",
      "sourceMaps": true,
      "runtimeArgs": ["--disable-web-security", "--user-data-dir"],
      "sourceMapPathOverrides": {
        "webpack:///*": "${webRoot}/src/components/*"
      }
    }
  ]
}
